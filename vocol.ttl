@prefix :        <http://vocol.vagrantshare.com/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<http://vocol.vagrantshare.com> rdf:type owl:Ontology .

:Vehicle
      rdf:type owl:Class ; 
      rdfs:comment "Transportation Mean"@en ;
      rdfs:label "Vehicle"@en .

:Bus  rdf:type owl:Class ;
      rdfs:comment "Bus"@en;
      rdfs:label "Bus"@en ;
      rdfs:subClassOf :Vehicle .

:MiniBus  rdf:type owl:Class ;
      rdfs:comment "MiniBus"@en;
      rdfs:label "MiniBus"@en ;
      rdfs:subClassOf :Bus .

:Bus1 rdf:type :Bus ;
      rdfs:label "Bus 1"@en .

:MiniBus1 rdf:type :MiniBus ;
      rdfs:label "Bus"@en .
