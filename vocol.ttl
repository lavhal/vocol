@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix : <http://vocab.cs.uni-bonn.de/unistruct#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xs: <http://www.w3.org/2001/XMLSchema#> .

<http://vocab.cs.uni-bonn.de/unistruct>
    a owl:Ontology .

:Bus
    a owl:Class ;
    rdfs:comment "Bus"@en ;
    rdfs:label "MinBus"@en ;
    rdfs:subClassOf :Vehicle .

:Car
    a owl:Class ;
    rdfs:comment "Car"@en ;
    rdfs:label "Car"@en ;
    rdfs:subClassOf :Vehicle .

:CompactCar
    a :Car ;
    rdfs:label "CompactCar"@en .

:Train
    a owl:Class ;
    rdfs:comment "Train Concept"^^xs:string ;
    rdfs:label "Train"^^xs:string ;
    rdfs:subClassOf :Vehicle .

:Vehicle
    a owl:Class ;
    rdfs:comment "Vehicle"@en ;
    rdfs:label "Vehicle"@en .

