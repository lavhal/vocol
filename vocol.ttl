@prefix : <http://vocab.cs.uni-bonn.de/unistruct#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xs: <http://www.w3.org/2001/XMLSchema#> .

<http://vocab.cs.uni-bonn.de/unistruct> a owl:Ontology .

:Vehicle rdf:type owl:Class ;
         rdfs:comment "Vehicle"@en;
         rdfs:label "Vehicle"@en .

:Bus     rdf:type owl:Class ;
         rdfs:comment "Bus"@en;
         rdfs:label "Bus"@en ;
         rdfs:subClassOf :Vehicle .

:MiniBus rdf:type :Bus ;
         rdfs:label "MiniBus"@en .
         
:Car     rdf:type owl:Class ;
         rdfs:comment "Car"@en;
         rdfs:label "Car"@en ;
         rdfs:subClassOf :Vehicle .
         
:CompactCar rdf:type :Car ;
rdfs:label "CompactCar"@en .
